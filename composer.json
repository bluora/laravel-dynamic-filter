{
    "name": "hnhdigital-os/laravel-model-filter",
    "description": "Provides model attribute filtering.",
    "keywords": ["laravel","illuminate","eloquent", "model", "filtering"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "hnhdigital-os/laravel-html-generator": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "illuminate/database": "4.*|5.*",
        "symfony/process": "~2.3",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "HnhDigital\\LaraveModelFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\LaravelModelFilter\\ServiceProvider"
            ]
        }
    }
}
